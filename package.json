{
    "name": "obsidian-daily-bookmarks",
    "version": "1.0.0",
    "type": "module",
    "description": "Plugin allows you to display a panel of favorite links and edit them. It also supports daily notes with calendar. Your data is stored only locally and only in your Obsidian",
    "main": "dist/index.js",
    "scripts": {
        "start": "rollup -c -w",
        "dev": "rollup -c --watch",
        "build": "rollup -c",
        "postinstall": "yarn build"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.18.0",
        "@mui/material": "^5.14.0",
        "@reduxjs/toolkit": "^2.8.2",
        "marked": "^4.0.0",
        "react": "^18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "^18.2.0",
        "react-redux": "^9.2.0",
        "react-window": "^1.8.11",
        "react-window-infinite-loader": "^1.0.10"
    },
    "devDependencies": {
        "@babel/core": "^7.22.10",
        "@babel/preset-react": "^7.22.5",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.3",
        "rollup": "^3.28.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-styles": "^4.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 safari version"
        ]
    }
}
