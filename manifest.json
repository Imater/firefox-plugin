{
    "manifest_version": 3,
    "name": "WebDAV Bookmarks",
    "version": "1.0",
    "description": "WebDAV Bookmarks Side Panel",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "side_panel": {
        "default_path": "sidebar.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "sidePanel",
        "storage",
        "webRequest",
        "tabs",
        "contextMenus",
        "notifications",
        "windows"
    ],
    "host_permissions": [
        "https://imater74.keenetic.link/*",
        "file://*/*"
    ]
}